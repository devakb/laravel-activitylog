{
    "name": "devakb/laravel-activitylog",
    "description": "A very simple activity logger to monitor the users of your website or application",
    "keywords": [
        "Devakb",
        "log",
        "user",
        "activity",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Amit Kumar Biswas",
            "email": "amit2x16@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "illuminate/config": "^6.0 || ^7.0 || ^8.0",
        "illuminate/database": "^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "ext-json": "*",
        "orchestra/testbench": "^4.0 || ^5.0 || ^6.0",
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devakb\\Activitylog\\ActivitylogServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Devakb\\Activitylog\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Devakb\\Activitylog\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
